{
  "name": "reprogleorg",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "echo \"Please run either start:dev or start:prod to begin development server based on environment\"",
    "start:dev": "ng serve --host 0.0.0.0 --configuration=development",
    "start:prod": "export NODE_ENV=prod && pnpm run config && ng serve --host 0.0.0.0 --configuration=production",
    "build": "pnpm run config && ng build",
    "watch": "ng build --watch --configuration development --host 0.0.0.0",
    "test": "ng test",
    "config": "ts-node scripts/setenv.ts",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.5",
    "@angular/common": "^15.1.5",
    "@angular/compiler": "^15.1.5",
    "@angular/core": "^15.1.5",
    "@angular/forms": "^15.1.5",
    "@angular/platform-browser": "^15.1.5",
    "@angular/platform-browser-dynamic": "^15.1.5",
    "@angular/router": "^15.1.5",
    "ngx-google-analytics": "^14.0.1",
    "ngx-turnstile": "^0.0.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.5.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.1.6",
    "@angular/compiler-cli": "^15.1.5",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "autoprefixer": "^10.4.13",
    "cypress": "^12.6.0",
    "eslint": "^8.34.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  }
}
